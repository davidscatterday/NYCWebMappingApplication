        <div class="bottomPanel">
            <div class="form-group" style="margin-bottom: 5px;">
                <div class="col-sm-11">
                    <div id="divSelectItemsCount"></div>
                    <div id="divSelectItemsMoreInfo" style="display: none">Select record to zoom on the map</div>
                </div>
                <div class="col-sm-1">
                    <button id="btnOpenSaveReport" type="button" class="btn btn-primary btn-sm" onclick="btnOpenSaveReport_Click()">Save Report</button>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-12">
                    <div id="divSelectItemsTable" style="max-height: 304px; overflow: auto;"></div>
                    <div id="divSelectItemsMessage" style="font-size: 14px; color: red; display: none">There are no selected records</div>
                </div>
            </div>
