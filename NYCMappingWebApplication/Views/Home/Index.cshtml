<script>
    GeometryServiceUrl = "@(System.Configuration.ConfigurationManager.AppSettings["GeometryServiceUrl"])";
    NYCbasemapUrl = "@(System.Configuration.ConfigurationManager.AppSettings["NYCbasemapUrl"])";
    MapPlutoUrl = "@(System.Configuration.ConfigurationManager.AppSettings["MapPlutoUrl"])";
    DistrictsUrl = "@(System.Configuration.ConfigurationManager.AppSettings["DistrictsUrl"])";
    CensusTractsUrl = "@(System.Configuration.ConfigurationManager.AppSettings["CensusTractsUrl"])";
    $(function () {
        $(".leftcolumn").resizable(
            {
                autoHide: true,
                handles: 'e',
                resize: function (e, ui) {
                    var parent = ui.element.parent();
                    var greenboxspace = parent.width() - ui.element.outerWidth(),
                        redbox = ui.element.next(),
                        redboxwidth = (greenboxspace - (redbox.outerWidth() - redbox.width())) / parent.width() * 100 + "%";
                    redbox.width(redboxwidth);
                },
                stop: function (e, ui) {
                    var parent = ui.element.parent();
                    ui.element.css(
                        {
                            width: ui.element.width() / parent.width() * 100 + "%",
                        });
                }
            });
    });
</script>
