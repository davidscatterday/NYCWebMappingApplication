    <div class="modal fade" id="myModalSaveReport" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content" style="width: 670px; height: 150px">
                <div class="modal-body">
                    <div class="form-group">
                        <label class="col-sm-3 control-label" style="margin-top: 5px;">Report Name</label>
                        <div class="col-sm-9">
                            @Html.TextBox("txtReportName", null, new { @class = "form-control input-width-xlarge", autocomplete = "off" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-offset-3 col-sm-9">
                            <button id="btnSaveMyReport" type="button" class="btn btn-primary btn-sm" style="margin-top: 10px;" onclick="btnSaveMyReport_Click()">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $("#txtBoroughs").select2({
        minimumInputLength: 0,  // minimumInputLength for sending ajax request to server
        width: '100%',   // to adjust proper width of select2 wrapped elements
        allowClear: true,
        multiple: true,
        ajax: {
            url: RootUrl + 'Home/GetBoroughs',
            type: "POST",
            dataType: 'json',
            data: function (term) {
                return {
                    term: term,
                };
            },
            results: function (data, page) {
                return {
                    results: data.BoroughsList,
                };
            }
        }
    });
</script>