@model NYCMappingWebApp.Entities.DataConsumerProfiles

@{
    Layout = "~/Views/Shared/_LayoutPages.cshtml";
    string infoMessage = TempData["InfoMessage"] as string;
    string errorMessage = TempData["ErrorMessage"] as string;
    string headerMessage = TempData["HeaderMessage"] as string;
}
<script>
    $(document).ready(function () {
        if (localStorage.getItem('ConsumerProfileSearchedDemographics') != null && localStorage.getItem('ConsumerProfileSearchedDemographics') != "null") {
            document.getElementById("ConsumerProfileSearchedDemographics").innerHTML = localStorage.getItem('ConsumerProfileSearchedDemographics');
        }
        if (localStorage.getItem('ConsumerProfileSearchedSocial') != null && localStorage.getItem('ConsumerProfileSearchedSocial') != "null") {
            document.getElementById("ConsumerProfileSearchedSocial").innerHTML = localStorage.getItem('ConsumerProfileSearchedSocial');
        }
        if (localStorage.getItem('ConsumerProfileSearchedEconomic') != null && localStorage.getItem('ConsumerProfileSearchedEconomic') != "null") {
            document.getElementById("ConsumerProfileSearchedEconomic").innerHTML = localStorage.getItem('ConsumerProfileSearchedEconomic');
        }
        if (localStorage.getItem('ConsumerProfileSearchedHousing') != null && localStorage.getItem('ConsumerProfileSearchedHousing') != "null") {
            document.getElementById("ConsumerProfileSearchedHousing").innerHTML = localStorage.getItem('ConsumerProfileSearchedHousing');
        }
    });
</script>
@if (infoMessage != null)
{
    <div class="form-group">
        <div class="alert alert-success fade in">
            @infoMessage
        </div>
    </div>
}
@if (errorMessage != null)
{
    <div class="form-group">
        <div class="alert alert-danger fade in">
            @errorMessage
        </div>
    </div>
}
@if (headerMessage != null)
{
    <div class="form-group">
        <div class="alert alert-info fade in">
            @Html.Raw(headerMessage)
            <button id="btnOpenMyReportConsumerProfile" type="button" style="margin-left: 7px; float: right" class="btn btn-primary btn-sm" onclick="btnOpenMyReportConsumerProfile_Click()">Save Report</button>
        </div>
    </div>
}

